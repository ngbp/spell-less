{
  "name": "warlock-spell-less",
  "version": "1.0.0-alpha.1",
  "description": "An addon to add LESS CSS support to the Warlock spell for managing client-side webapps.",
  "homepage": "https://github.com/ngbp/spell-less",
  "author": {
    "name": "Warlock Team",
    "url": "http://getwarlock.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ngbp/spell-less.git"
  },
  "bugs": {
    "url": "https://github.com/ngbp/spell-less"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/ngbp/spell-less/blob/master/LICENSE"
    }
  ],
  "engines": {
    "node": ">= 0.10.0"
  },
  "peerDependencies": {
    "warlock-spell-webapp": "~1.0.0"
  },
  "keywords": [
    "warlockplugin",
    "warlockspell"
  ],
  "warlock": {
    "globs": {
      "source": {
        "less": [
          "<%= paths.source_app %>/**/*.less"
        ]
      }
    },
    "tasks": {
      "less-compile": {
        "dumpLineNumbers": "comments"
      }
    }
  },
  "dependencies": {
    "gulp-less": "^1.2.3"
  }
}
